{
  "name": "candymachine_assets",
  "version": "0.1.0",
  "description": "Internet Computer starter application",
  "keywords": [
    "Internet Computer",
    "Motoko",
    "JavaScript",
    "Canister"
  ],
  "scripts": {
    "postinstall": "cd canisters/candymachine && npm install",
    "build": "webpack",
    "prebuild": "npm run copy:types",
    "start": "webpack serve --mode development --env development",
    "prestart": "npm run copy:types",
    "copy:types": "rsync -avr .dfx/$(echo ${DFX_NETWORK:-'**'})/canisters/** --exclude='assets/' --exclude='idl/' --exclude='*.wasm' --exclude='*.most' --delete canisters/declarations"
  },
  "devDependencies": {
    "@dfinity/agent": "^0.11.1",
    "@dfinity/candid": "^0.11.1",
    "@dfinity/principal": "^0.11.1",
    "assert": "2.0.0",
    "buffer": "6.0.3",
    "copy-webpack-plugin": "^9.0.1",
    "events": "3.3.0",
    "html-webpack-plugin": "5.5.0",
    "process": "0.11.10",
    "sass": "^1.54.9",
    "sass-loader": "^13.0.2",
    "stream-browserify": "3.0.0",
    "terser-webpack-plugin": "5.2.5",
    "ts-loader": "^9.3.0",
    "typescript": "^4.6.4",
    "util": "0.12.4",
    "webpack": "^5.63.0",
    "webpack-cli": "4.9.1",
    "webpack-dev-server": "^4.4.0"
  },
  "browserslist": [
    "last 2 chrome version",
    "last 2 firefox version",
    "last 2 safari version",
    "last 2 edge version"
  ],
  "dependencies": {
    "@dfinity/auth-client": "^0.11.1",
    "@dfinity/authentication": "^0.11.1",
    "@dfinity/identity": "^0.11.1",
    "@psychedelic/dab-js": "^1.3.2",
    "@psychedelic/plug-connect": "^0.1.4",
    "azle": "^0.6.0",
    "bootstrap": "^5.1.3",
    "css-loader": "^6.7.1",
    "jquery": "^3.6.0",
    "react": "^18.1.0",
    "react-bootstrap": "^2.2.0",
    "react-bootstrap-drawer": "^1.0.0",
    "react-bootstrap-typeahead": "^6.0.0-alpha.9",
    "react-countdown": "^2.3.2",
    "react-dom": "^18.1.0",
    "react-router-dom": "^6.3.0",
    "recoil": "^0.7.2",
    "resolve-cwd": "^3.0.0",
    "style-loader": "^3.3.1"
  }
}
